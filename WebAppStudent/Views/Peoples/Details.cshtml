@model WebAppStudent.Models.People
@{
  ViewData["Title"] = "Detalhes";
}
<h1>Detalhes</h1>
<div>
  <h4>Pessoa</h4>
  <hr />
  <dl class="row">
    <dt class="col-sm-2">
      Id
    </dt>
    <dd class="col-sm-10">
      @Html.DisplayFor(model => model.Id)
    </dd>
    <dt class="col-sm-2">
      Nome
    </dt>
    <dd class="col-sm-10">
      @Html.DisplayFor(model => model.Name)
    </dd>
    <dt class="col-sm-2">
      Ativo
    </dt>
    <dd class="col-sm-10">
      @Html.DisplayFor(model => model.Active)
    </dd>
  </dl>
</div>
<div>
  @Html.ActionLink("Alterar", "Edit", new { id = Model.Id }, new { @class = "btn btn-primary" }) | <a asp-action="Index" class="btn btn-info">Voltar</a>
</div>
